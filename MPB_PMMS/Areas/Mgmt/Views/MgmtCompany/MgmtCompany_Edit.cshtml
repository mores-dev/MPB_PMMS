@model MPB_Entities.Mgmt.MgmtCompany_EditMain
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width" />
    <title>航商資料管理</title>
    <!-- 3rd part css -->
    <link href="~/Content/Styles/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Scripts/bootstrap-5.0.2-dist/css/bootstrap.min.css" rel="stylesheet" />
    <!-- 3rd part js -->
    <script src="~/Scripts/jquery.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery-ui-1.12.1/jquery-ui.js" type="text/javascript"></script>
    <link href="~/Scripts/jquery-ui-1.12.1/jquery-ui.css" rel="stylesheet" type="text/css" />   
    <script src="~/Scripts/jquery.vs.func.js" type="text/javascript"></script>
    <!-- main style -->
    <link href="~/Content/Styles/style.css" rel="stylesheet" />
    <!-- Chosen Plugin  -->
    <link href="~/Scripts/chosen-1.8.7/chosen.css?2019062411:46:43" rel="stylesheet" />
    <script src="~/Scripts/chosen-1.8.7/chosen.jquery.js?2019062411:46:43"></script>
    <script src="~/Scripts/chosen-1.8.7/chosen.proto.js?2019062411:46:43"></script>
    <!-- handsontable Plugin  -->
    <link href="~/Content/hot/handsontable.full.css" rel="stylesheet" />
    <link href="~/Content/hot/handsontable.full.custom.css" rel="stylesheet" />
    <script src="~/Scripts/hot/handsontable.full.js?2019062411:46:43"></script>
    <script src="~/Scripts/jquery.vs.handsontable.js?2019062411:46:43"></script>    
    <!-- workcode  -->
    <script src="~/Scripts/workcode/Mgmt/MgmtCompany_Edit.js?2020030311:46:43"></script>
    <script>
        var isEditMode      =@ViewBag.isEditMode;
        var isViewMode      =@ViewBag.isViewMode;
        var isDeleteMode    =@ViewBag.isDeleteMode;
        var mode    ='@ViewBag.mode';
        const tokenVal = '@RequestVerificationTokenHelper.TokenValue()';
        @Html.Raw((ViewBag.JsScript))
    </script>
</head>
<body style="height:100%">
    @{ Html.BeginForm(); }
    @Html.AntiForgeryToken()
    <input type="hidden" id="mode" value="@ViewBag.mode" />
    <input type="hidden" id="currentUrl" value="@ViewBag.currentUrl" />
    <input type="hidden" id="mod_key">
    <input type="hidden" id="C_ID" value="@Model.C_ID">
    <input type="hidden" id="chk_Value">

    <h1 class="pageTitle">航商資料管理 - @ViewBag.titleStr</h1>
    <div class="sub-content">
        <div class="row">
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <div class="form-group">
                    <label id="Label3" class="control-label">*航商代號</label>
                    @Html.TextBox("C_CODE", @Model.C_CODE, new { @class = "vs_initialForm({M:10}) vs_formValidate({NE:1}) form-control", title = "航商代號", autocomplete = "off"  })
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <div class="form-group">
                    <label id="Label3" class="control-label">*航商名稱</label>
                    @Html.TextBox("C_NAME", @Model.C_NAME, new { @class = "vs_initialForm({M:20}) vs_formValidate({NE:1}) form-control", title = "航商名稱", autocomplete = "off"  })
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <div class="form-group">
                    <label id="Label3" class="control-label">*航商名稱_英文</label>
                    @Html.TextBox("C_NAME_EN", @Model.C_NAME_EN, new { @class = "vs_initialForm({M:20}) vs_formValidate({NE:1}) form-control", title = "航商名稱_英文", autocomplete = "off"  })
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <div class="form-group">
                    <label id="Label4" class="control-label">統一編號</label>
                    @Html.TextBox("C_TAX_ID", @Model.C_TAX_ID, new { @class = "vs_initialForm({M:8}) form-control", title = "統一編號", autocomplete = "off"  })
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <div class="form-group">
                    <label id="Label3" class="control-label">*聯絡電話</label>
                    @Html.TextBox("Phone", @Model.Phone, new { @class = "vs_initialForm({M:20}) vs_formValidate({NE:1}) form-control", title = "聯絡電話", autocomplete = "off"  })
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <div class="form-group">
                    <label id="Label3" class="control-label">*地址</label>
                    @Html.TextBox("C_Address", @Model.C_Address, new { @class = "vs_initialForm({M:100}) vs_formValidate({NE:1}) form-control", title = "地址", autocomplete = "off"  })
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <div class="form-group">
                    <label id="Label3" class="control-label">EMAIL</label>
                    @Html.TextBox("Email", @Model.Email, new { @class = "vs_initialForm({M:100}) form-control", title = "EMAIL", autocomplete = "off"  })
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <div class="form-group">
                    <label id="Label3" class="control-label">訂票電話</label>
                    @Html.TextBox("Odr_Phone", @Model.Odr_Phone, new { @class = "vs_initialForm({M:20}) form-control", title = "訂票電話", autocomplete = "off"  })
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <div class="form-group">
                    <label id="Label3" class="control-label">官網網址</label>
                    @Html.TextBox("LAND_URL", @Model.LAND_URL, new { @class = "vs_initialForm({M:200}) form-control", title = "官網網址", autocomplete = "off"  })
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <div class="form-group">
                    <label id="Label3" class="control-label">票價網址</label>
                    @Html.TextBox("Price_URL", @Model.Price_URL, new { @class = "vs_initialForm({M:200}) form-control", title = "票價網址", autocomplete = "off"  })
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <div class="form-group">
                    <label id="Label3" class="control-label">訂票網站</label>
                    @Html.TextBox("Odr_URL", @Model.Odr_URL, new { @class = "vs_initialForm({M:200}) form-control", title = "訂票網站", autocomplete = "off"  })
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <div class="form-group">
                    <label id="Label3" class="control-label">Logo網址</label>
                    @Html.TextBox("Logo_URL", @Model.Logo_URL, new { @class = "vs_initialForm({M:200}) form-control", title = "Logo網址", autocomplete = "off"  })
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <div class="form-group">
                    <label id="Label3" class="control-label">*狀態</label>
                    <select class="chosen-select vs_formValidate({NE:1})   " data-placeholder="請選擇..." id="C_STATUS" name="C_STATUS" title="狀態">
                        @Html.Raw(ViewBag.StatusHtml)
                    </select>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <label id="Label3" class="control-label">*為必填欄位</label>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="form-group">
                    @Html.DisplayName("CreateDt")@Model.CreateDt　@Html.DisplayName("CreateId")@Model.CreateId　　@Html.DisplayName("ModifyDt")@Model.ModifyDt　@Html.DisplayName("ModifyId")@Model.ModifyId
                </div>
            </div>
        </div>
        <div class="row">
            <hr />
            <div class="col-xs-12 col-sm-6 col-md-5 col-lg-5">
                <div class="form-group">
                    <input type="button" id="btnBack" class="btn btn-default btn-lg" value="取　消">
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-7 col-lg-7">
                <div class="form-group">
                    <input type="button" id="btnConfirm" class="btn btn-confirm btn-lg" value="存　檔">
                </div>
            </div>
        </div>
    </div> <!-- end sub-content -->
    @{Html.EndForm(); }
</body>
</html>